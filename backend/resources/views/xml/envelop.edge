<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
  xmlns:web="http://localhost/recupera/webservice/">
  <soapenv:Header />
  <soapenv:Body>
    <web:IncluirOcorrencia>
      <web:strXML>
        <![CDATA[
            <XML>
              <OCORRENCIA>
                  <CODIGO>{{action.codigo}}</CODIGO>
                  <CREDOR>{{action.credor}}</CREDOR>
                  <REGIS>{{action.regis}}</REGIS>
                  <OPERADOR>{{credentials.UID}}</OPERADOR>
                  <COMPLEMENTO>{{action.complemento}}</COMPLEMENTO>
                  <DATAAGENDA />
                  <FONEDISCADO>{{action.fonediscado}}</FONEDISCADO>
                  <CODCONTRATOVINCULAR>{{action.cocontratovincular}}</CODCONTRATOVINCULAR>
                  <CODPRODUVINCULAR>1</CODPRODUVINCULAR>
                  <BLOQUEIOCONTRATO>FALSE</BLOQUEIOCONTRATO>
              </OCORRENCIA>
              <login>
                  <UID>{{credentials.UID}}</UID>
                  <PWD>{{credentials.PWD}}</PWD>
                  <EMPRESA>AEGEA</EMPRESA>
              </login>
            </XML>
          ]]>
      </web:strXML>
    </web:IncluirOcorrencia>
  </soapenv:Body>
</soapenv:Envelope>
