{
  "extends": "@adonisjs/tsconfig/tsconfig.app.json",
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./build",
    "baseUrl": ".",
    "sourceMap": true,
    "paths": {
      "#models/*": [
        "app/models/*"
      ],
      "#controllers/*": [
        "app/controllers/http/*"
      ],
      "#providers/*": [
        "app/providers/*"
      ],
      "#services/*": [
        "app/services/*"
      ],
      "#middlewares/*": [
        "app/middleware/*"
      ],
      "#helpers/*": [
        "app/helpers/*"
      ],
      "#validators/*": [
        "app/validators/*"
      ],
      "#jobs/*": [
        "app/jobs/*"
      ],
      "#sql/*": [
        "app/sql/*"
      ],
      "#utils/*": [
        "app/utils/*"
      ],
      "#routes/*": [
        "start/routes/*"
      ],
      "#start/*": [
        "start/*.js"
      ],
      "#config/*": [
        "config/*.js"
      ]
    }
  },
}